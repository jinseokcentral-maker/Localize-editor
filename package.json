{
  "name": "localeeditor",
  "version": "0.0.0",
  "description": "Excel-like i18n translation editor built with AG Grid",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "i18n",
    "translation",
    "editor",
    "excel",
    "grid",
    "ag-grid",
    "internationalization"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:lib && pnpm build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "build:lib": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check:outdated": "pnpm outdated"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.10.7",
    "@vitest/ui": "^4.0.15",
    "jsdom": "^27.3.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.15"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/lodash": "^4.17.21",
    "effect": "^3.19.13",
    "jspreadsheet-ce": "^4.15.0",
    "jsuites": "^5.2.0",
    "lodash": "^4.17.21",
    "marked": "^17.0.1",
    "tailwindcss": "^4.1.18",
    "zod": "^4.2.1"
  }
}
